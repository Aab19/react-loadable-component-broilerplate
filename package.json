{
  "name": "react-loadable-broilerplate",
  "version": "1.0.0",
  "description": "broilerplate react js with express js use loadable-componente to split component and redux",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"yarn webpack:watch\" \"yarn server:watch\"",
    "webpack:watch": "rimraf assets/build && cross-env NODE_ENV=development webpack --watch --progress",
    "server:watch": "nodemon --watch src/ --watch assets/ --exec \"node -r dotenv/config -r babel-register\" ./src/server/index"
  },
  "author": "Asih Priyatno",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "express": "^4.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "assets-webpack-plugin": "^3.9.10",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.1",
    "dotenv": "^8.1.0",
    "nodemon": "^1.19.2",
    "rimraf": "^3.0.0",
    "webpack": "^4.40.0",
    "webpack-cli": "^3.3.8"
  }
}
